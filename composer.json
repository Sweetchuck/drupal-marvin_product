{
    "type": "drupal-drush",
    "name": "drupal/marvin_product",
    "description": "Drush commands to manage contrib development",
    "license": "GPL-3.0-or-later",
    "keywords": [
        "drupal",
        "drush"
    ],
    "homepage": "https://github.com/Sweetchuck/drupal-marvin_product",
    "authors": [
        {
            "name": "DÃ¡vid Andor",
            "role": "Maintainer",
            "homepage": "https://github.com/Sweetchuck"
        }
    ],
    "support": {
        "source": "https://github.com/Sweetchuck/drupal-marvin_product",
        "issues": "https://github.com/Sweetchuck/drupal-marvin_product/issues"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true,
            "sweetchuck/composer-repo-path": true,
            "sweetchuck/composer-suite": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "danielstjules/stringy": "source",
            "*": "dist"
        },
        "sort-packages": true
    },
    "repositories": {
        "drupal/marvin": {
            "type": "git",
            "url": "https://github.com/Sweetchuck/drupal-marvin.git"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "consolidation/robo": "^3.0",
        "drupal/marvin": "2.x-dev",
        "sweetchuck/utils": "1.x-dev"
    },
    "require-dev": {
        "behat/mink-goutte-driver": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
        "drupal/core-composer-scaffold": "^9.0",
        "drupal/core-recommended": "^9.0",
        "drush/drush": "^11.0",
        "mikey179/vfsstream": "^1.6",
        "nuvoleweb/robo-config": "2.x-dev",
        "oomphinc/composer-installers-extender": "^2.0",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.5",
        "sweetchuck/composer-repo-path": "1.x-dev",
        "sweetchuck/composer-suite": "1.x-dev",
        "symfony/phpunit-bridge": "^5.3",
        "weitzman/drupal-test-traits": "2.x-dev"
    },
    "suggest": {
        "drush/drush": "^11.0"
    },
    "conflict": {
        "drupal/marvin_incubator": "*"
    },
    "autoload": {
        "psr-4": {
            "Drush\\Commands\\marvin_product\\": "Commands/marvin_product/",
            "Drupal\\marvin_product\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\marvin_product\\": "tests/src/",

            "Drupal\\Tests\\": "tests/fixtures/project_01/docroot/core/tests/Drupal/Tests/",
            "Drupal\\TestTools\\": "tests/fixtures/project_01/docroot/core/tests/Drupal/TestTools/",
            "Drupal\\KernelTests\\": "tests/fixtures/project_01/docroot/core/tests/Drupal/KernelTests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-8.x-1.x": "1.x-dev",
            "dev-9.x-2.x": "2.x-dev"
        },
        "drush": {
            "services": {
                "drush.services.yml": "^9"
            }
        },
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patches": {
            "danielstjules/stringy": {
                "https://github.com/danielstjules/Stringy/issues/209 - PHP 8.1 compatibility": "https://patch-diff.githubusercontent.com/raw/danielstjules/Stringy/pull/211.patch"
            },
            "drupal/core": {
                "https://www.drupal.org/project/drupal/issues/3182103 - PhpUnit typehints": "https://www.drupal.org/files/issues/2022-12-28/drupal-3182103-0905-39-phpunit-typehints.patch"
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "tests/fixtures/project_01/docroot/core": [
                "type:drupal-core"
            ],
            "tests/fixtures/project_01/docroot/libraries/{$name}": [
                "type:bower-asset",
                "type:npm-asset",
                "type:drupal-library"
            ],
            "tests/fixtures/project_01/docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "tests/fixtures/project_01/docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "tests/fixtures/project_01/docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "tests/fixtures/project_01/drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "tests/fixtures/project_01/docroot"
            },
            "file-mapping": {
                "[web-root]/modules/.gitignore": {
                    "mode": "skip"
                },
                "[web-root]/modules/README.txt": {
                    "mode": "skip"
                },
                "[web-root]/profiles/.gitignore": {
                    "mode": "skip"
                },
                "[web-root]/profiles/README.txt": {
                    "mode": "skip"
                },
                "[web-root]/themes/.gitignore": {
                    "mode": "skip"
                },
                "[web-root]/themes/README.txt": {
                    "mode": "skip"
                },
                "[web-root]/sites/example.settings.local.php": {
                    "mode": "skip"
                },
                "[web-root]/sites/.gitignore": {
                    "mode": "skip"
                },
                "[web-root]/sites/README.txt": {
                    "mode": "skip"
                },
                "[web-root]/.csslintrc": {
                    "mode": "skip"
                },
                "[web-root]/.editorconfig": {
                    "mode": "skip"
                },
                "[web-root]/.eslintignore": {
                    "mode": "skip"
                },
                "[web-root]/.eslintrc.json": {
                    "mode": "skip"
                },
                "[web-root]/.gitattributes": {
                    "mode": "skip"
                },
                "[web-root]/.gitignore": {
                    "mode": "skip"
                },
                "[web-root]/example.gitignore": {
                    "mode": "skip"
                },
                "[web-root]/INSTALL.txt": {
                    "mode": "skip"
                },
                "[web-root]/README.txt": {
                    "mode": "skip"
                },
                "[web-root]/.htaccess": {
                    "mode": "skip"
                },
                "[web-root]/web.config": {
                    "mode": "skip"
                },
                "[project-root]/.editorconfig": {
                    "mode": "skip"
                }
            },
            "initial": {
                "sites/default/default.services.yml": "sites/default/services.yml",
                "sites/default/default.settings.php": "sites/default/settings.php"
            }
        },
        "marvin": {
            "drushUnish": {
                "filesToSymlink": {
                    "etc": true
                }
            }
        }
    },
    "scripts": {
        "post-install-cmd": [
            "\\Drupal\\Tests\\marvin_product\\Helper\\ComposerScripts::postInstallCmd"
        ],
        "post-update-cmd": [
            "\\Drupal\\Tests\\marvin_product\\Helper\\ComposerScripts::postUpdateCmd"
        ],
        "clean": [
            "rm -rf bin/ reports/ vendor/",
            "git clean --force -d -x -- tests/",
            "git clean --force -d -X -- tests/"
        ],
        "test": [
            "rm -rf ./reports/",
            "@test:unit",
            "@test:integration"
        ],
        "test:unit": [
            "mkdir -p reports/machine/coverage-php/",
            "XDEBUG_MODE=coverage phpunit --testsuite Unit --coverage-html reports/human/coverage/Unit/html --testdox-html reports/human/testdox/Unit.html --coverage-clover reports/machine/coverage-clover/Unit.xml --coverage-php reports/machine/coverage-php/Unit.php --log-junit reports/machine/junit/Unit.xml"
        ],
        "test:integration": [
            "mkdir -p reports/machine/coverage-php/",
            "phpunit --testsuite Integration --coverage-html reports/human/coverage/Integration/html --testdox-html reports/human/testdox/Integration.html --coverage-clover reports/machine/coverage-clover/Integration.xml --coverage-php reports/machine/coverage-php/Integration.php --log-junit reports/machine/junit/Integration.xml"
        ],
        "generate:coverage-html": [
            "\\Drupal\\Tests\\marvin_product\\Helper\\ComposerScripts::generateCoverageHtml"
        ]
    }
}
